{
  "name": "resume-cred-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"docker-compose up\" \"npx prisma migrate dev --name init\" \"npx prisma generate\" \"next dev\" \"ts-node src/app/api/test_redis/server.ts\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "redis": "nodemon redis-pub-sub/server.js",
    "ws": "nodemon websocket-server/server.js",
    "seed-db": "cd src/db && node seedDB.ts",
    "format:fix": "npx prettier --write \"**/*.{ts,tsx,json}\"",
    "format:check": "npx prettier --check \"**/*.{ts,tsx,json}\"",
    "postinstall": "npm install --prefix websocket-server && npm install --prefix redis-pub-sub"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "dependencies": {
    "@prisma/client": "5.18.0",
    "@sd-jwt/core": "^0.6.1",
    "@sd-jwt/crypto-nodejs": "^0.6.1",
    "@sd-jwt/sd-jwt-vc": "^0.6.1",
    "@types/ws": "^8.5.10",
    "antd": "^5.17.0",
    "axios": "^1.7.2",
    "file-type": "19.0.0",
    "html2pdf.js": "^0.10.1",
    "jose": "^5.3.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "jwt-decode": "^4.0.0",
    "next": "14.2.1",
    "next-auth": "^4.24.7",
    "nodemon": "^3.1.3",
    "qrcode": "^1.5.3",
    "react": "^18",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-json-view-lite": "^1.4.0",
    "redis": "^4.6.15",
    "uuid": "^9.0.1",
    "ws": "^8.17.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.0.11",
    "prisma": "5.18.0",
    "typescript": "^5"
  },
  "prisma": {
    "packageManager": "npm"
  }
}
